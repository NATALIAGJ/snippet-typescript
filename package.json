{
  "name": "snippet-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "tsc": "tsc",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles",
    "test:watch": "cross-env NODE_ENV=test jest --detectOpenHandles --watch .",
    "start": "node dist/server.js",
    "build:typescript": "rm -rf ./dist/ && tsc",
    "build": "npm run clean && cross-env NODE_ENV=production npm run build:typescript",
    "clean": "rm -rf dist && rm -rf public/dist && rm -rf ./logs/**",
    "watch": "concurrently \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/server.js",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "docker-bp": "",
    "deploy": "npm run build && npm run docker-bp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NATALIAGJ/snippet-typescript.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NATALIAGJ/snippet-typescript/issues"
  },
  "homepage": "https://github.com/NATALIAGJ/snippet-typescript#readme",
  "dependencies": {
    "@types/cors": "^2.8.7",
    "@types/figlet": "^1.2.0",
    "@types/jest": "^26.0.9",
    "@types/mongoose": "^5.7.34",
    "@types/multer": "^1.4.3",
    "@types/slug": "^0.9.1",
    "@types/supertest": "^2.0.10",
    "bcrypt": "^5.0.0",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "currently": "0.0.8",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "figlet": "^1.5.0",
    "jest": "^26.2.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.27",
    "multer": "^1.4.2",
    "rm": "^0.1.8",
    "slug": "^3.3.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "ts-node-dev": "^1.0.0-pre.56",
    "uuid-base62": "^0.1.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/fs-extra": "^9.0.1",
    "@types/node": "^14.0.27",
    "@types/shelljs": "^0.8.8",
    "fs-extra": "^9.0.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "json",
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts)"
    ],
    "testEnvironment": "node"
  }
}
